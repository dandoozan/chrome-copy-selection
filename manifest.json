{
    "name": "CopySelected",
    "version": "0.1",
    "description": "",
    "permissions": ["tabs", "*://*/*", "clipboardWrite"],
    "manifest_version": 2,
    "browser_action": {},
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "exclude_matches": ["https://docs.google.com/*"],
            "js": ["js/toast.js", "js/execScript.js"],
            "css": ["css/toast.css"]
        }
    ],
    "background": {
        "scripts": ["js/background_listenForReload.js"],
        "persistent": false
    }
}
